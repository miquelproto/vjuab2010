//----------------------------------------------------------------------------
// Init data
//----------------------------------------------------------------------------
bool  CRenderableObject::Init()
{
  m_bIsOk = true;

  if (!m_bIsOk){
    Release();
  }

  return m_bIsOk;
}

//----------------------------------------------------------------------------
// Finalize data
//----------------------------------------------------------------------------
void CRenderableObject::Done()
{
  if (IsOk())
  {
    Release();
    m_bIsOk = false;
  }
}

